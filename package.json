{
  "name": "kahootclient",
  "productName": "KahootClient",
  "version": "1.1.0",
  "description": "Spielerich lernen",
  "main": "src/main.js",
  "scripts": {
    "start": "electron ."
  },
  "build": {
    "appId": "com.lslabs.kahootclient",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "src/icon.ico"
